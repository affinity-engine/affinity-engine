{{#each layerDirectables as |directable|}}
  {{component directable.componentPath
    directable=directable
    keyboardActivated=keyboardActivated
    theaterId=theaterId
    windowId=windowId
  }}
{{/each}}

{{#each-in childLayers as |name directables|}}
  {{ember-theater/director/layer
    directables=directables
    keyboardActivated=keyboardActivated
    name=name
    theaterId=theaterId
    windowId=windowId
  }}
{{/each-in}}
